{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "src",
  "moduleNameMapper": {
    "@categories/(.*)": "<rootDir>/src/categories/$1",
    "@videos/(.*)": "<rootDir>/src/videos/$1"
  },
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  },
  "collectCoverageFrom": ["**/*.(t|j)s"],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
